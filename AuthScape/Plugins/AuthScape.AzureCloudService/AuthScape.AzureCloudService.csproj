    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <PackageId>AuthScape.AzureCloudService</PackageId>
    <Title>AuthScape.AzureCloudService</Title>
    <Version>1.2.194</Version>
    <Authors>$(AssemblyName)ZuechB</Authors>
