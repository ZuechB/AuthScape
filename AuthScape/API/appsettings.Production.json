{
  "AppSettings": {
    "Name": "Authscape",
    "Stage": 3,
    "IDPUrl": "https://localhost:44303",
    "WebsiteRedirectUrl": "http://localhost:3000",
    "InviteSignupRedirectUrl": "http://localhost:3000",
    "LoginRedirectUrl": "http://localhost:3000",
    "DataProtectionTokenProviderOptions_TokenLifespanByDays": 1,
    "EnableCompanyMode": true, // Will display Company within signup and also store company against payment processes vs user level
    "Storage": {
      "AzureConnectionString": "AzureStorageConnectionString",
      "BaseUri": "https://storage.website.com",
      "UserProfileContainer": "userphoto"
    },
    "Mapping": {
      "AzureConnectionString": "AzureStorageConnectionString",
      "BaseUri": "https://blobName.blob.core.windows.net",
      "Container": "containerName"
    },
    "Stripe": {
      "PublishableKey": "PKStripe",
      "SecretKey": "SecretKeyStripe",
      "SigningSecret": "SigningSecretKey"
    },
    "Ticketing": {
      "TemplateId": "sendgridTemplateId",
      "Domain": "subdomain.companyname.com",
      "Name": "CompanyName",
      "Subject": "New Message"
    },
    "OpenAI": {
      "APIKey": ""
    },
    "LuceneSearch": {
      "StorageConnectionString": "AzureStorageConnectionString",
      "Container": "azureContainerName"
    },
    "Spreadsheet": {
      "AzureVisionKey": "visionKey",
      "AzureVisionEndpoint": "https://visionname.cognitiveservices.azure.com"
    },
    "Reporting": {
      "ProjectName": "Reports" // This is the assembly name of your project
    },
    "DocumentProcessing": {
      "StorageContainer": "mapping",
      "BaseURL": "https://yoursite.blob.core.windows.net",
      "AzureFormRecognizerEndpoint": "https://namehere.cognitiveservices.azure.com/",
      "AzureFormRecognizerKey": "keyhere"
    },
    "PrivateLabel": {
      "GoogleFontsAPIKey": "googlefontkey",
      "AppIconContainer": "appicons",
      "AppServicePlanName": "AppServicePlanName",
      "WebAppName": "WebAppName",
      "ResourceGroupName": "ResourcesGroupName",
      "SubscriptionId": "SubscriptionGuidId",
      "OpenIdApplicationId": "OpenIdGuidId",
      "ClientId": "ClientId",
      "ClientSecret": "SecretId",
      "TenantId": "TenantId"
    },
    "FirebaseBrowserPushNotification_APIKey": "",
    "SendGrid": {
      "APIKey": "sendGridAPIKey",
      "FromEmail": "hello@companyname.com",
      "FromName": "CompanyName"
    },
    "SendGridWebhook": {
      "NewEmail": "new@mydomain.com" // This is for new emails coming into the system
    },
    "Authentication": {
      "EnableCompany": true,
      "SearchCompany": true
    },
    "Slack": {
      "UserName": "botName",
      "IconUrl": "IconPath",
      "WebhookUrl": "https://hooks.slack.com/services/[yourcode]"
    },
    "DatabaseContext": "Data Source=localhost;Initial Catalog=AuthScape;Integrated Security=true;Trusted_Connection=true;TrustServerCertificate=true;"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*"
}